import java.util.Scanner;
class Largestarray{
	public static void main(String[] qwe){
	Scanner scan=new Scanner(System.in);
	int N=scan.nextInt();
	int arr[]=new int[N];
	int max=0;
    for(int i=0;i<N;i++)
	arr[i]=scan.nextInt();
	max=arr[0];
	for(int i=0;i<N;i++){
		if(max<arr[i])
		max=arr[i];
	}
	System.out.println(max);
	}
}
