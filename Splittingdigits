import java.util.Scanner;
class Splittingdigits{
public static void main(String[] qwe){
Scanner scan=new Scanner(System.in);
int N=scan.nextInt();
int a=0,b=0,c=0;
if((N>=100)&&(N<=999)){
		a=N/100;
		N=N%100;
		System.out.print(a+" ");
		b=N/10;
		System.out.print(b+" ");
	    c=N%10;
	    System.out.print(c+" ");
	}
else if((N>=0)&&(N<=99)){
        b=N/10;
		System.out.print(b+" ");
	    c=N%10;
	    System.out.print(c+" ");
}
}
}
