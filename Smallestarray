import java.util.Scanner;
class Smallestarray{
	public static void main(String[] qwe){
	Scanner scan=new Scanner(System.in);
	int N=scan.nextInt();
	int arr[]=new int[N];
	int min=0;
    for(int i=0;i<N;i++)
	arr[i]=scan.nextInt();
	min=arr[0];
	for(int i=0;i<N;i++){
		if(min>arr[i])
		min=arr[i];
	}
	System.out.println(min);
	}
}
