import java.util.Scanner;
import java.lang.Math;
class Powerof2{
public static void main(String[] args){
Scanner scan=new Scanner(System.in);
int N=scan.nextInt();
int count=0;
for(int i=0;i<N;i++){
if(N==java.lang.Math.pow(2,i))
count=count+1;}
if(count==1)
System.out.println("yes");
else
System.out.println("no");
}
}
