import java.util.Scanner;
class Maxoftennumbers{
	public static void main(String[] qwe){
		Scanner scan=new Scanner(System.in);
		int arr[]=new int[10];
		int max=0;
		for(int i=0;i<10;i++){
			arr[i]=scan.nextInt();
		}
		max=arr[0];
		for(int i=0;i<10;i++){
			if(max<arr[i])
			max=arr[i];
		}
		System.out.println(max);
	}
}
