import java.util.Scanner;
class MinimumAndMaximumArray{
public static void main(String[] qwe){
Scanner scan=new Scanner(System.in);
int N=scan.nextInt();
int arr[]=new int[N];
int max=0,min=0;
for(int i=0;i<N;i++)
arr[i]=scan.nextInt();
min=arr[0];
for(int i=0;i<N;i++){
if(min>arr[i])
min=arr[i];
}
System.out.print(min+" ");
max=arr[0];
for(int i=0;i<N;i++){
if(max<arr[i])
max=arr[i];
}
System.out.print(max+" ");
}
}
