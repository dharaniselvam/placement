import java.util.Scanner;
class Armstrong{
	public static void main(String[] qwe){
	Scanner scan=new Scanner(System.in);
	int N=scan.nextInt();
	int temp=0,r=0,mul=0;
	temp=N;
    while(N!=0){
	r=N%10;
	N=N/10;
	mul=mul+(r*r*r);
    }
    if(mul==temp)
    System.out.println("yes");
    else 
    System.out.println("no");
	}
	}


	
	
