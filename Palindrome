import java.util.Scanner;
class Palindrome{
public static void main(String[] qwe){
Scanner scan=new Scanner(System.in);
int N=scan.nextInt();
int temp=0;
int r,sum=0;
temp=N;
while(N>0){
	r=N%10;
	N=N/10;
	sum=(sum*10)+r;
	
}
if(temp==sum)
System.out.println("yes");
else
System.out.println("no");
}
}
		
