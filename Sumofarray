import java.util.Scanner;
class Sumofarray{
	public static void main(String[] args){
		Scanner scan=new Scanner(System.in);
		int N=scan.nextInt();
		int K=scan.nextInt();
		int a[]=new int[N];
		int sum=0;
		for(int i=0;i<K;i++){
			a[i]=scan.nextInt();
			sum=sum+a[i];
		}
		System.out.println(sum);
	
}
}
